{
  "editor.minimap.enabled": false,
  "editor.lineNumbers": "relative",
  "editor.cursorStyle": "line",
  "zenMode.fullScreen": true,
  "zenMode.restore": true,
  "editor.formatOnSave": true,
  "extensions.autoUpdate": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.showTabs": false,
  "extensions.ignoreRecommendations": false,
  "workbench.editor.showIcons": false,
  "window.zoomLevel": 0,
  "files.insertFinalNewline": true,
  "workbench.colorTheme": "Atom One Light",
  "editor.fontFamily": "JetBrains Mono",
  "vim.disableExtension": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": true,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": false,
    "suppressWelcomeNotice": true
  },
  "editor.renderWhitespace": "all",
  "gitlens.codeLens.scopes": ["document", "containers"],
  "vim.overrideCopy": false,
  "gitlens.keymap": "none",
  "javascript.validate.enable": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "cSpell.userWords": [
    "Flic",
    "Rollbar",
    "Tricorder",
    "bruteforce",
    "euctricorder",
    "precompiled",
    "productboard",
    "roadmap"
  ],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "plaintext": "jade"
  },
  "cSpell.language": "en",
  "editor.suggest.localityBonus": true,
  "breadcrumbs.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.editor.highlightModifiedTabs": true,
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": false,
    "source.organizeImports": false
  },
  "tslint.alwaysShowRuleFailuresAsWarnings": false,
  "problems.decorations.enabled": false,
  "editor.suggestSelection": "first",
  "editor.suggest.showSnippets": true,
  "editor.snippetSuggestions": "bottom",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.associations": {
    "*.css": "css"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {},
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "liveshare.featureSet": "insiders",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.rulers": ["85"],
  "codemetrics.basics.OverviewRulerModeEnabled": false,
  "workbench.activityBar.visible": true,
  "gitlens.menus": {
    "editor": {
      "blame": false,
      "clipboard": true,
      "compare": true,
      "details": false,
      "history": false,
      "remote": false
    },
    "editorGroup": false,
    "editorTab": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "explorer": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "scmGroup": {
      "compare": true,
      "openClose": true,
      "stash": true,
      "stashInline": true
    },
    "scmItem": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true,
      "stash": true
    }
  },
  "codemetrics.basics.CodeLensEnabled": false,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.iconTheme": "vscode-icons",
  "workbench.sideBar.location": "right",
  "editor.fontSize": 13,
  "terminal.integrated.fontFamily": "Operator Mono",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.shell.osx": "/usr/local/bin/fish"
}
